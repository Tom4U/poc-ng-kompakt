{
  "name": "club-app",
  "version": "1.0.0",
  "description": "Ein simple Vereinsseite",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "predeploy": "rimraf dist",
    "deploy": "copyfiles --flat src/**/*.html src/**/*.css dist",
