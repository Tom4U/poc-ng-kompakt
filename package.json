    "url": "https://github.com/Tom4U/uebung-ng-kompakt/issues"
  },
  "homepage": "https://github.com/Tom4U/uebung-ng-kompakt#readme",
  "devDependencies": {
    "concurrently": "^7.4.0",
    "copyfiles": "^2.4.1",
    "lite-server": "^2.6.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.4"
  }
}
